<div class="container">
  <div>
    <h1>Message Details</h2>
  </div>
  <%= link_to 'Back', form_responses_path(@form), class: 'btn btn-info' %>
  <div class="table-responsive">
    <table class="table">
      <tr>
        <% @fields.each do |field| %>
          <th><%= field.label %></th>
        <% end %>
      </tr>
      <tr>
        <% @responses.each do |field| %>
          <td><%= field.value %></td>
        <% end %>
      </tr>
    </table>
  </div>
</div>